{
    "name": "popphp/pop-queue",
    "description": "Pop Queue Component for Pop PHP Framework",
    "keywords": [
        "queue",
        "job",
        "worker",
        "scheduler",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "http://www.popphp.org/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "popphp/popphp": "^4.0.0",
        "popphp/pop-db": "^6.0.0",
        "popphp/pop-utils": "^2.0.0",
        "opis/closure": "^3.6.3",
        "ext-redis": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Queue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Queue\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
