{
    "name": "popphp/pop-queue",
    "description": "Pop Queue Component for Pop PHP Framework",
    "keywords": [
        "queue",
        "job",
        "worker",
        "scheduler",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "https://github.com/popphp/pop-queue",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@noladev.com",
            "homepage": "https://www.popphp.org/"
        }
    ],
    "require": {
        "php": ">=8.3.0",
        "popphp/popphp": "^4.3.8",
        "popphp/pop-db": "^6.6.5",
        "popphp/pop-utils": "^2.3.0",
        "laravel/serializable-closure": "^2.0.3",
        "aws/aws-sdk-php": "^3.283.18"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0.0"
    },
    "suggest": {
        "ext-redis": "For using the Redis adapter"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Queue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Queue\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
